[core]
	repositoryformatversion = 0
	filemode = true
	bare = true
[status]
	showUntrackedFiles = no
[remote "origin"]
	url = https://github.com/Guilhermatheus/dotfiles.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[push]
	autoSetupRemote = yes
[branch "main"]
	remote = origin
	merge = refs/heads/main
[alias]
	unstage = reset HEAD --
	last = log -1 HEAD
	update = "!f() { git add -u && git commit -m \"$@\"; }; f"
	files = "ls-tree -r main --name-only"
